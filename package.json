{
  "name": "obsidian-markdown-formatting-assistant-plugin",
  "version": "0.4.0",
  "description": "This plugin improves the writing of MarkDown files",
  "homepage": "https://github.com/Reocin/obsidian-markdown-formatting-assistant-plugin",
  "main": "main.js",
  "scripts": {
    "dev": "rollup --config rollup.config.js -w && openObsidian",
    "dev2": "nodemon --config nodemon.json",
    "openObsidian": "/usr/bin/flatpak run --branch=stable --arch=x86_64 md.obsidian.Obsidian \"$@\"",
    "build": "rollup --config rollup.production.config.js"
  },
