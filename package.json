  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.4",
    "@types/node": "^18.6.4",
    "@types/ramda": "^0.28.15",
    "nodemon": "^2.0.19",
    "rollup": "^2.77.2",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "repository": "git@github.com:Reocin/obsidian-markdown-autocomplete.git",
  "dependencies": {
    "@mdi/js": "^7.0.96",
    "@rollup/plugin-json": "^4.1.0",
    "obsidian": "^0.15.9",
    "postcss": "^8.4.16",
    "ramda": "^0.28.0",
